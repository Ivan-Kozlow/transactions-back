{
	"version": 2,
	"env": {
		"PORT": "@PORT",
		"HOST": "@HOST",
		"JWT_SECRET": "@JWT_SECRET",
		"DB_NAME": "@DB_NAME",
		"DB_USERNAME": "@DB_USERNAME",
		"DB_PASSWORD": "@DB_PASSWORD"
	},
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "DELETE"]
		}
	]
}
